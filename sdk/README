export JAVA_OPTS="-Dlog.level=error"

Generator command for python:
openapi-generator-cli generate -g python -o python/sdk -i openapi_schema.json --skip-validate-spec

Generator command for csharp:
openapi-generator-cli generate -g csharp -o csharp/sdk -i openapi_schema.json --skip-validate-spec

Generator command for java:
openapi-generator-cli
generate -g java -o java/sdk -i openapi_schema.json --artifact-version 2.0.0 --skip-validate-spec -c java/java.conf